    "cors": "^2.8.1",
    "debug": "^2.2.0",
    "express": "~4.14.0",
    "jade": "~1.11.0",
    "morgan": "~1.7.0",
    "pg": "^6.1.0",
    "pg-promise": "^5.4.3",
    "request": "^2.79.0",
    "serve-favicon": "~2.3.0",
    "stormpath": "^0.19.0"
  },
  "devDependencies": {
    "blanket": "^1.2.3",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.2.0",
    "mocha-junit-reporter": "^1.12.1",
    "sinon": "^1.17.6"
  },
  "config": {
    "blanket": {
